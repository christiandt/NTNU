  <requirements xsi:type="jex:ExercisePart" xmi:id="_requirements_1" text="The Account Interface">
    <requirements xsi:type="jex:JavaRequirement" xmi:id="_requirements_1_requirements_0" text="public interface Account in package oving6" javaElement="Account_javaElements_0_classes_2">
      <requirements xsi:type="jex:JavaRequirement" xmi:id="_requirements_1_requirements_0_requirements_0" text="public int method getBalance in public interface Account in package oving6" javaElement="getBalance_javaElements_0_classes_2_members_0">
        <description><div xmlns='http://www.w3.org/1999/xhtml' class='description' id='_requirements_1_requirements_0_requirements_0_description'>Takes no parameters and returns the current <b>balance</b> of this <code>Account</code> as an <code>int</code>.</div></description>
      </requirements>
      <requirements xsi:type="jex:JavaRequirement" xmi:id="_requirements_1_requirements_0_requirements_1" text="public int method getCredit in public interface Account in package oving6" javaElement="getCredit_javaElements_0_classes_2_members_1">
        <description><div xmlns='http://www.w3.org/1999/xhtml' class='description' id='_requirements_1_requirements_0_requirements_1_description'>Takes no parameters and returns the granted <b>credit</b> of this <code>Account</code>, as an <code>int</code>. If no credit is granted it should return zero.</div></description>
      </requirements>
      <requirements xsi:type="jex:JavaRequirement" xmi:id="_requirements_1_requirements_0_requirements_2" text="public int method deposit in public interface Account in package oving6" javaElement="deposit_javaElements_0_classes_2_members_2">
        <description><div xmlns='http://www.w3.org/1999/xhtml' class='description' id='_requirements_1_requirements_0_requirements_2_description'>Takes an <code>int</code> as a parameter, increases the <b>balance</b> of this <code>Account</code> and returns the new balance. If the parameter is negative, nothing should happen (the balance should still be returned).</div></description>
      </requirements>
      <requirements xsi:type="jex:JavaRequirement" xmi:id="_requirements_1_requirements_0_requirements_3" text="public int method withdraw in public interface Account in package oving6" javaElement="withdraw_javaElements_0_classes_2_members_3">
        <description><div xmlns='http://www.w3.org/1999/xhtml' class='description' id='_requirements_1_requirements_0_requirements_3_description'>Takes an <code>int</code> as a parameter, decreases (if allowed) the <b>balance</b> of this <code>Account</code> and returns the amount that is made available from the Account. If the requested amount is not covered or it is negative, no money should be withdrawn and zero should be returned.</div></description>
      </requirements>
      <description><div xmlns='http://www.w3.org/1999/xhtml' class='description' id='_requirements_1_requirements_0_description'>Define the interface Account with the following method declarations:</div></description>
    </requirements>
